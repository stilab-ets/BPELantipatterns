         <bpel:documentation>Receive the initial request for an estimate from the presentation tier of the application.&#13;
</bpel:documentation>
      </bpel:receive>
      <bpel:scope aei:implicit="true" name="Validate_Request_Using_Rules_Engine">
         <bpel:variables>
            <bpel:variable messageType="qrules:InvokeClaimRulesRequest" name="parameters"/>
         </bpel:variables>
         <bpel:sequence>
            <bpel:assign aei:id="133001768496902591" name="Prepare_Request_Validation">
               <bpel:copy aei:id="133001768496902593">
                  <bpel:from>$claimRequest</bpel:from>
                  <bpel:to part="Document" variable="parameters">
                     <bpel:query>claim:claimRequest</bpel:query>
                  </bpel:to>
               </bpel:copy>
            </bpel:assign>
            <bpel:invoke aei:id="133001768496802585" inputVariable="parameters" name="Validate_Request_Using_Rules_Engine" operation="InvokeClaimRules" outputVariable="ClaimRulesOutput" partnerLink="Rules_Service"/>
         </bpel:sequence>
      </bpel:scope>
      <bpel:if aei:id="133001768497002595" name="Is_the_request_valid">
         <bpel:documentation>Did the rules service determine that the claim request is valid?</bpel:documentation>
         <bpel:condition>$ClaimRulesOutput/qrulesd:IsValid = 'true'</bpel:condition>
         <bpel:sequence aei:id="133001768497102601">
            <bpel:scope aei:implicit="true" name="Reply_With_Claim_Number">
               <bpel:variables>
                  <bpel:variable messageType="ns1:ClaimResponse" name="parameters"/>
               </bpel:variables>
               <bpel:sequence>
                  <bpel:assign aei:id="133001768497602633">
                     <bpel:copy aei:id="133001768497602635">
                        <bpel:from expressionLanguage="urn:active-endpoints:expression-language:xquery1.0"><![CDATA[<claim:claimAck xmlns:claim="http://schemas.active-endpoints.com/demo/claimRequest/2010/02/ClaimRequest.xsd">
   <claim:refNumber>{ $refNumber}</claim:refNumber>
   <claim:claimDate>{ current-date()}</claim:claimDate>
   <claim:claimStatus>pending</claim:claimStatus>
</claim:claimAck>
]]></bpel:from>
                        <bpel:to part="response" variable="parameters"/>
                     </bpel:copy>
                  </bpel:assign>
                  <bpel:reply aei:id="133001768497402623" name="Reply_With_Claim_Number" operation="ClaimRequestOperation" partnerLink="Customer" variable="parameters">
                     <bpel:correlations>
                        <bpel:correlation initiate="yes" set="CS1"/>
                     </bpel:correlations>
                  </bpel:reply>
               </bpel:sequence>
            </bpel:scope>
            <bpel:scope aei:id="133001768497202603">
